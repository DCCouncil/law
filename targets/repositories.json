{
    "repositories": {
        "DCCouncil/law-docs": {
            "custom": {
                "allow-unauthenticated-commits":true,
                "serve":"latest",
                "location_regex":"~/.*\\.pdf$",
                "routes": [
                    ".*\\.pdf"
                ]
            }
        },
        "DCCouncil/law-xml": {
            "custom": {
                "type":"xml",
                "allow-unauthenticated-commits":true,
                "serve":"latest",
                "serve-prefix":"_uncodified_xml"
            }
        },
        "DCCouncil/law-html": {
            "custom": {
                "preview":"git@github.com:DCCouncil/law-html-preview.git",
                "type":"html",
                "serve":"historical",
                "location_regex":"/",
                "routes": [
                    ".*"
                ]
            }
        },
        "DCCouncil/law-xml-codified": {
            "custom": {
                "preview":"git@github.com:DCCouncil/law-xml-codified-preview.git",
                "type":"xml-codified",
                "serve":"historical",
                "serve-prefix":"_xml"
            }
        },
        "DCCouncil/law-static-assets": {
            "custom": {
                "type":"static-assets",
                "allow-unauthenticated-commits":true,
                "serve":"latest",
                "location_regex":"~.+(_document|_reader).+$",
                "is_fallback": true,
                "routes": [
                    ".*/_document/.*",
                    "_reader/.*"
                ]
            }
        },
        "DCCouncil/law-rdf": {
            "custom": {
                "serve": "latest",
                "type": "rdf",
                "serve-prefix": "_rdf"
            }
        }
    }
}